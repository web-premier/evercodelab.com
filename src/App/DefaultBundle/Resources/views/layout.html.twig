    'js/jquery-1.10.2.min.js'
    'js/jquery.slides.min.js'
    'js/jquery.fs.scroller.min.js'
    'js/main.js'
    output='compiled/compiled.js'
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}

