<!doctype html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name='yandex-verification' content='609f107a156157ec' />
    <meta name="google-site-verification" content="IOomL4TBfTjjsuUqlcEkKH_YWCniNPOKNw0BpIW1WvI" />
    <meta name="KEYWORDS" content="{{ 'site.keywords'|trans }}" />
    <meta name="DESCRIPTION" content="{{ 'site.description'|trans }}" />
    <title>{{ 'site.title'|trans }}</title>
    {% stylesheets
        'css/style.css'
        'css/custom.css'
        output='compiled/compiled.css'
        filter='cssrewrite'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
    {% endstylesheets %}
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <![endif]-->

    <link rel="icon" href="{{ asset('favicon.ico') }}" type="image/x-icon" />
    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" type="image/x-icon" />
</head>
<body>
<div class="wrapper">
    <!-- Верхняя навигация -->
    <header class="container">
        <div class="logo">
            <a href="{{ path('index') }}"><img src="/img/logo.png" alt="EvercodeLab"></a>
            <a href="{{ path('index') }}" class="name"><h1>Evercode Lab</h1></a>
        </div>
        <div class="menu">
            {{ knp_menu_render('AppDefaultBundle:Builder:mainMenu', {template: 'AppDefaultBundle::main_menu.html.twig', currentClass: 'active'}) }}
<!--             {{ knp_menu_render('AppDefaultBundle:Builder:languageMenu', {template: 'AppDefaultBundle::language_menu.html.twig', currentClass: 'active'}) }}
 -->        </div>
    </header>

    <!-- Псевдоэлементы -->
    <div class="before-left-block"><i></i></div>
    <div class="before-right-block"><i></i></div>

    {% block content %}
    {% endblock %}

    <!-- Подвал -->
    <footer class="clear">
        <div class="container">
            <div class="social">
                <a href="https://www.facebook.com/pages/Evercode-Lab/128121020625049" class="sprite fb"></a>
                <a href="https://github.com/EvercodeLab" class="sprite gh"></a>
                <a href="http://twitter.com/evercodelab" class="sprite tw"></a>
                <a href="http://feeds.feedburner.com/EvercodeBlog" class="sprite rss"></a>
            </div>
            <img src="/img/logo.png" alt="">
            <div class="design">
                дизайн сайта<br/>
                <a href="http://formazon.com">formazon.com</a>
            </div>
        </div>
    </footer>

    <div class="contacts" id="contact_right">
        <p class="contact-trigger">контакты</p>
        <div class="number">
            <p>8 921 426 34 22</p>
            <p>hello@evercodelab.com</p>
            <p>Ответим обязательно!</p>
        </div>
    </div>
</div>


