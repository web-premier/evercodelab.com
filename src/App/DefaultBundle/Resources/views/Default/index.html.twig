<p>Pages</p>
{% for page in pages %}
<a href="{{ path('page_show', {'slug':page.slug }) }}"><li> {{ page.title }} </li></a>
{% else %}
No page have been found.
{% endfor %}
<p>Clients</p>
{% for client in clients %}
<li> {{ client.name }} </li>
{% else %}
No client have been found.
{% endfor %}
<p>Portfolio</p>
{% for portfolio in portfolios %}
<li> {{ portfolio.name }} </li>
{% else %}
No client have been found.
{% endfor %}